<robot name="Carousel">
	<link name="base_link">
		<visual name="base" with_col="true">
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.2"/>
			</geometry>
			<material>
				<color rgba="0.5 0 0 1"/>
			</material>
		</visual>

		<inertial>
			<mass value="5"/>
		</inertial>
	</link>
	
	<link name="head_link">
		<visual name="tilt" with_col="true">
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.06" length="0.10"/>
			</geometry>
			<material name="">
			<color rgba="0 0 0 1"/>
			</material>
		</visual>
	</link>
	
	<joint name="motor1" type="continuous" visible="true">
		<origin xyz="0 0 0.25" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="head_link"/>
		<axis xyz="0.0 0.0 1.0"/>
		<limit effort="10.0" velocity="5.0"/>
	</joint>
	
	<link name="arm_link">
		<visual name="arm" with_col="true">
			<origin xyz="0.1 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.3 0.05 0.03"/>
			</geometry>
			<material>
				<color rgba="0.5 0.5 0 1"/>
			</material>
		</visual>
		<visual name="shaft">
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
			<geometry>
				<cylinder radius="0.01" length="0.1"/>
			</geometry>
			<material name="">
			
			</material>
		</visual>
		<inertial>
			<mass value="0.1"/>
		</inertial>
	</link>
	
	<joint name="tilt_joint" type="revolute" visible="true">
		<origin xyz="0.05 0 0" rpy="0 0 0"/>
		<parent link="head_link"/>
		<child link="arm_link"/>
		<axis xyz="0.0 1.0 0.0"/>
		<limit lower="-1" upper="1.0" effort="10.0" velocity="1.0"/>
	</joint>
	
	
	<link name="swing">
		
		<visual name="seat" with_col="true">
			<origin xyz="0 0 -0.2" rpy="0 0 0"/>
			<geometry>
				<box size="0.04 0.04 0.01"/>
			</geometry>
			<material name="">
			<color rgba="0.5 1 0 1"/>
			</material>
		</visual>
		
		<visual name="bar1">
			<origin xyz="0 0 -0.1" rpy="0 0 0"/>
			<geometry>
				<box size="0.005 0.005 0.2"/>
			</geometry>
			<material name="">
			
			</material>
		</visual>

	
		<inertial>
			<mass value="0.1"/>
		</inertial>

	</link>

	<joint name="free_rotate" type="pin" visible="true">
		<origin xyz="0.24 0 0" rpy="0 0 0"/>
		<parent link="arm_link"/>
		<child link="swing"/>
	</joint>

	
	
</robot>
