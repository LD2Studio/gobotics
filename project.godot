; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gobotics"
run/main_scene="res://game/game.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://gobotics_logo.svg"

[display]

window/size/viewport_width=1600
window/size/viewport_height=800

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/actuator3d/plugin.cfg", "res://addons/control_camera3d/plugin.cfg")

[importer_defaults]

scene={
"_subresources": {},
"animation/fps": 30,
"animation/import": true,
"animation/remove_immutable_tracks": true,
"animation/trimming": false,
"blender/animation/always_sample": true,
"blender/animation/group_tracks": true,
"blender/animation/limit_playback": true,
"blender/materials/export_materials": 1,
"blender/materials/unpack_enabled": true,
"blender/meshes/colors": false,
"blender/meshes/export_bones_deforming_mesh_only": false,
"blender/meshes/normals": true,
"blender/meshes/skins": 2,
"blender/meshes/tangents": true,
"blender/meshes/uvs": true,
"blender/nodes/cameras": true,
"blender/nodes/custom_properties": true,
"blender/nodes/modifiers": 1,
"blender/nodes/punctual_lights": true,
"blender/nodes/visible": 0,
"gltf/embedded_image_handling": 1,
"import_script/path": "res://game/blender_import/import_scene.gd",
"meshes/create_shadow_meshes": true,
"meshes/ensure_tangents": true,
"meshes/generate_lods": true,
"meshes/light_baking": 1,
"meshes/lightmap_texel_size": 0.2,
"nodes/apply_root_scale": true,
"nodes/root_name": "Scene Root",
"nodes/root_scale": 1.0,
"nodes/root_type": "Node3D",
"skins/use_named_skins": true
}

